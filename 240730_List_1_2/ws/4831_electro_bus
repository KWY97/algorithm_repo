T = int(input()) # 노선 수

for tc in range(1, T+1):
    K, N, M = map(int, input().split())
    charging_station = list(map(int, input().split())) # M개의 충전소 위치 
    arr = [0 for _ in range(N+1)] # 0번 ~ N번 정류장
    
    for x in charging_station: 
        arr[x] = 1 # 충전이 가능한 곳은 리스트의 해당 인덱스의 값을 1로
    
    charging_cnt = 0
    
